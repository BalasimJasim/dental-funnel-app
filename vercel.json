{
  "buildCommand": "cd client && VITE_DEBUG=true npm install && npm run build",
  "outputDirectory": "client/dist",
  "framework": "vite",
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 