{
  "buildCommand": "cd client && VITE_LANGUAGE=uk NODE_ENV=production npm run build",
  "outputDirectory": "client/dist",
  "framework": "vite",
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_LANGUAGE": "uk",
    "NODE_ENV": "production"
  }
} 